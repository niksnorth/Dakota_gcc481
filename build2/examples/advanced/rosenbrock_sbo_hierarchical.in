
environment,
## Activate Dakota's legacy X Windows-based graphics on Unix systems
## Consider newer capabilities in the Dakota GUI
	graphics
	tabular_data
	top_method_pointer = 'SBLO'

method,
	id_method = 'SBLO'
	surrogate_based_local
	model_pointer = 'SURROGATE'
	method_pointer = 'NLP'
	trust_region
	  initial_size = 0.10
	  contract_threshold = 0.25
	  expand_threshold = 0.75
	  contraction_factor = 0.50
	  expansion_factor   = 1.50

method,
	id_method = 'NLP'
## (NPSOL requires a software license; if not available, try
## conmin_frcg or optpp_newton instead)
	npsol_sqp
	  max_iterations = 50
	  convergence_tolerance = 1e-10

model,
	id_model = 'SURROGATE'
	surrogate hierarchical
	  ordered_model_fidelities = 'LOFI' 'HIFI'
	  correction additive       second_order

variables,
	continuous_design = 2
	  initial_point   -1.2      1.0
	  lower_bounds    -2.0     -2.0
	  upper_bounds     2.0 	2.0
	  descriptors      'x1'     'x2'

responses,
	objective_functions = 1
 	analytic_gradients
	analytic_hessians

model,
	id_model = 'LOFI'
	single
	  interface_pointer = 'LOFI_FN'

interface,
	id_interface = 'LOFI_FN'
  analysis_drivers = 'lf_rosenbrock'
	  direct
  deactivate restart_file

model,
	id_model = 'HIFI'
	single
	  interface_pointer = 'HIFI_FN'

interface,
	id_interface = 'HIFI_FN'
  analysis_drivers = 'rosenbrock'
    direct
  deactivate restart_file
