# Dakota Input File: textbook_uq_surrogate.in
environment
  tabular_data
    tabular_data_file = 'textbook_uq_surrogate.dat'
  top_method_pointer = 'UQ'

method
  id_method = 'UQ'
  sampling
    sample_type lhs
    samples = 1000
    seed = 98765
    response_levels = 0.1 0.2 0.6
                      0.1 0.2 0.6
                      0.1 0.2 0.6
    distribution cumulative
    model_pointer = 'SURR'

model
  id_model = 'SURR'
  surrogate global
    dace_method_pointer = 'DACE'
    reuse_points all
#    polynomial quadratic
#    neural_network
#    gaussian_process surfpack
    mars

method
  id_method = 'DACE'
  sampling sample_type lhs
    samples = 121 seed = 5034
    model_pointer = 'DACE_M'

model
  id_model = 'DACE_M'
  single
    interface_pointer = 'I1'

variables
  uniform_uncertain = 2
    lower_bounds =  0.   0.
    upper_bounds =  1.   1.
    descriptors  = 'x1' 'x2'

interface
  id_interface = 'I1'
  analysis_drivers = 'text_book'
    fork
  asynchronous evaluation_concurrency = 5

responses
  response_functions = 3
  no_gradients
  no_hessians
